{
  "version": 2,
  "name": "app",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist/spa" }
    }
  ],
  "routes": [
    {
      "src": "/(css|fonts|js|statics)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
